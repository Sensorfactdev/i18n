{
  "name": "@sensorfactdev/i18n",
  "version": "2.1.0",
  "description": "Internationalisation client for translations managed in GraphQL",
  "repository": {
    "type": "git",
    "url": "git@github.com:Sensorfactdev/i18n.git"
  },
  "license": "MIT",
  "scripts": {
